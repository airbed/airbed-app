<%= form_for @apartment, html: { multipart: true } do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :people %>
  <%= f.text_field :people %>

  <%= f.submit %>
<% end %>